<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    <h2>CSV Upload</h2>
    <ol class="breadcrumb">
      <li>
        <a href="/">Home</a>
      </li>
      <li>
        <a>CSV Upload</a>
      </li>
      <li class="active">
        <strong>CSV</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2">

  </div>
</div>

<% if @message.present? %>
    <div class="alert alert-info alert-dismissable">
      <button aria-hidden="true" data-dismiss="alert" class="close" type="button">Ã—</button>
      <%= @message %>
    </div>
<% end %>



<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h5>Listing Bookings</h5>
      </div>
      <div class="ibox-content">

        <table class="table table-bordered">
          <thead>
          <tr>
            <th>Customer</th>
            <th>Cleaner</th>
            <th>Date</th>
            <th colspan="3"></th>
          </tr>
          </thead>
          <tbody>
            <% @bookings.each do |booking| %>
      <tr>
        <td><%= link_to(booking.customer.full_name, booking.customer) %></td>
        <td><%= link_to(booking.cleaner.full_name, booking.cleaner) %></td>
        <td><%= booking.date %></td>
        <td><%= link_to 'Show', booking %></td>
        <td><%= link_to 'Edit', edit_booking_path(booking) %></td>
        <td><%= link_to 'Destroy', booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
          </tbody>
          <%#= will_paginate @bookings %>
        </table>

      </div>
    </div>
  </div>
</div>

<%= link_to 'New Booking', new_booking_path %>
